{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "array-bracket-spacing": "error",
    "arrow-spacing": "error",
    "complexity": "error",
    "curly": "off",
    "@next/next/no-img-element": "off",
    "no-buffer-constructor": "error",
    "no-var": "error"
  }
}
