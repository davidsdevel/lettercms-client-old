{
  "name": "@lettercms/client",
  "license": "Apache",
  "version": "0.1.0",
  "engines": {
    "yarn": ">= 1.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/lettercms/client.git"
  },
  "scripts": {
    "start": "node index",
    "start:dev": "node src/compile && next dev --port 3002",
    "start:vercel-dev": "node src/compile && next dev src/ --port 3002",
    "start:test": "set NODE_ENV=production&& node src/compile && next build src/ && node test",
    "compile": "node src/compile && next build src/",
    "build:dev": "set NODE_ENV=development&& node src/compile && next build src/",
    "build:prod": "webpack",
    "lint": "eslint -c .eslintrc.js .",
    "test": "jest --runInBand --detectOpenHandles",
    "vercel-build": "node src/compile && next build src/"
  },
  "main": "index.js",
  "dependencies": {
    "@lettercms/sdk": "^0.0.10",
    "@sentry/browser": "^5.22.3",
    "@sentry/integrations": "^5.22.3",
    "@sentry/node": "^5.22.3",
    "feed": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "next": "12",
    "react": "18",
    "react-dom": "18",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.10.3",
    "@lettercms/build-utils": "0.0.8",
    "eslint": "8",
    "eslint-config-next": "12.1.6",
    "jest": "^26.0.1",
    "react-refresh": "^0.8.3",
    "terser": "^4.8.0",
    "terser-webpack-plugin": "^4.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
